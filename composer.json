{
    "name": "cesargb/laravel-cascade-delete",
    "description": "Cascading eliminations implemented in polymorphic relationships for the Laravel apps",
    "keywords": [
        "laravel",
        "elocuent",
        "cascade",
        "delete",
        "polymorphic",
        "relation"
    ],
    "type": "library",
    "require": {
        "php": "^7.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/events": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Cesargb\\Database\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Cesar Garcia",
            "email": "cesargb@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
